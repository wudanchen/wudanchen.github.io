#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules:/home/wdc/mycode/vue_blog/my-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../@vuepress+plugin-redirect@2.0.0-rc.115_vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0._311d09cb1e6f43ce73d94f7563e04bf2/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
fi
